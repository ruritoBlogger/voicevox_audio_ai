{
  "name": "voicevox_audio_ai",
  "version": "0.0.0",
  "description": "voicevox + chatGPT でなんかする",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build && next export",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx}",
    "lint:fix": "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx} --fix",
    "format": "prettier src/**/*.{ts,tsx}",
    "format:fix": "prettier --write src/**/*.{ts,tsx}"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.json --no-eslintrc src/**/*.{ts,tsx} --fix",
      "prettier --write src/**/*.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@next/bundle-analyzer": "12.2.5",
    "@types/node": "18.11.13",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@emotion/css": "11.10.0",
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.17",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
